# shellcheck disable=SC2006

prefix='/workspaces/auth_capture_proxy/venv'
# Used in 'libdir'
# shellcheck disable=SC2034
exec_prefix="${prefix}"
# shellcheck disable=SC2034
includedir="${prefix}/include"

usage()
{
  cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared        says 'yes' if libcurl was built shared
  --ca                  CA bundle install path
  --cc                  compiler
  --cflags              preprocessor and compiler flags
  --checkfor [version]  check for (lib)curl of the specified version
  --configure           the arguments given to configure when building curl
  --features            newline separated list of enabled features
  --help                display this help and exit
  --libs                library linking information
  --prefix              curl install prefix
  --protocols           newline separated list of enabled protocols
  --ssl-backends        output the SSL backends libcurl was built to support
  --static-libs         static libcurl library linking information
  --version             output version information
  --vernum              output version as a hexadecimal number
EOF

  exit "$1"
}

if test "$#" -eq 0; then
  usage 1
fi

while test "$#" -gt 0; do
  case "$1" in
  --built-shared)
    echo 'yes'
    ;;

  --ca)
    echo '/workspaces/auth_capture_proxy/venv/ssl/cacert.pem'
    ;;

  --cc)
    echo '/home/conda/feedstock_root/build_artifacts/curl_split_recipe_1770892450256/_build_env/bin/x86_64-conda-linux-gnu-cc'
    ;;

  --prefix)
    echo "$prefix"
    ;;

  --feature|--features)
    for feature in alt-svc AsynchDNS GSS-API HSTS HTTP2 HTTPS-proxy IPv6 Kerberos Largefile libz NTLM SPNEGO SSL threadsafe TLS-SRP UnixSockets zstd ''; do
      test -n "$feature" && echo "$feature"
    done
    ;;

  --protocols)
    # shellcheck disable=SC2043
    for protocol in DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS MQTT POP3 POP3S RTSP SCP SFTP SMB SMBS SMTP SMTPS TELNET TFTP WS WSS; do
      echo "$protocol"
    done
    ;;

  --version)
    echo 'libcurl 8.18.0'
    exit 0
    ;;

  --checkfor)
    checkfor="$2"
    cmajor=`echo "$checkfor" | cut -d. -f1`
    cminor=`echo "$checkfor" | cut -d. -f2`
    # when extracting the patch part we strip off everything after a
    # dash as that's used for things like version 1.2.3-pre1
    cpatch=`echo "$checkfor" | cut -d. -f3 | cut -d- -f1`

    vmajor=`echo '8.18.0' | cut -d. -f1`
    vminor=`echo '8.18.0' | cut -d. -f2`
    # when extracting the patch part we strip off everything after a
    # dash as that's used for things like version 1.2.3-pre1
    vpatch=`echo '8.18.0' | cut -d. -f3 | cut -d- -f1`

    if test "$vmajor" -gt "$cmajor"; then
      exit 0
    fi
    if test "$vmajor" -eq "$cmajor"; then
      if test "$vminor" -gt "$cminor"; then
        exit 0
      fi
      if test "$vminor" -eq "$cminor"; then
        if test "$cpatch" -le "$vpatch"; then
          exit 0
        fi
      fi
    fi

    echo "requested version $checkfor is newer than existing 8.18.0"
    exit 1
    ;;

  --vernum)
    echo '081200'
    exit 0
    ;;

  --help)
    usage 0
    ;;

  --cflags)
    if test "${prefix}/include" = '/usr/include'; then
      echo ''
    else
      echo " -I${prefix}/include"
    fi
    ;;

  --libs)
    if test "${exec_prefix}/lib" != '/usr/lib' && test "${exec_prefix}/lib" != '/usr/lib64'; then
      curllibdir="-L${exec_prefix}/lib "
    else
      curllibdir=''
    fi
    if test 'yes' = 'no'; then
      echo "${curllibdir}-lcurl -lnghttp2 -lssh2 -lssh2 -lssl -lcrypto -lssl -lcrypto -L/workspaces/auth_capture_proxy/venv/lib -lgssapi_krb5 -lzstd -lzstd -lz -pthread"
    else
      echo "${curllibdir}-lcurl"
    fi
    ;;

  --ssl-backends)
    echo 'OpenSSL'
    ;;

  --static-libs)
    if test 'yes' != 'no'; then
      echo "${exec_prefix}/lib/libcurl.a -L/workspaces/auth_capture_proxy/venv/lib -L/workspaces/auth_capture_proxy/venv/lib -L/workspaces/auth_capture_proxy/venv/lib -L/workspaces/auth_capture_proxy/venv/lib -L/workspaces/auth_capture_proxy/venv/lib -lnghttp2 -lssh2 -lssh2 -lssl -lcrypto -lssl -lcrypto -L/workspaces/auth_capture_proxy/venv/lib -lgssapi_krb5 -lzstd -lzstd -lz -pthread"
    else
      echo 'curl was built with static libraries disabled' >&2
      exit 1
    fi
    ;;

  --configure)
    echo " '--prefix=/workspaces/auth_capture_proxy/venv' '--host=x86_64-conda-linux-gnu' '--disable-ldap' '--enable-websockets' '--with-ca-bundle=/workspaces/auth_capture_proxy/venv/ssl/cacert.pem' '--with-openssl=/workspaces/auth_capture_proxy/venv' '--with-zlib=/workspaces/auth_capture_proxy/venv' '--with-zstd=/workspaces/auth_capture_proxy/venv' '--with-gssapi=/workspaces/auth_capture_proxy/venv' '--with-libssh2=/workspaces/auth_capture_proxy/venv' '--with-nghttp2=/workspaces/auth_capture_proxy/venv' '--without-libpsl' 'build_alias=x86_64-conda-linux-gnu' 'host_alias=x86_64-conda-linux-gnu' 'CC=/home/conda/feedstock_root/build_artifacts/curl_split_recipe_1770892450256/_build_env/bin/x86_64-conda-linux-gnu-cc' 'CFLAGS=-march=nocona -mtune=haswell -ftree-vectorize -fPIC -fstack-protector-strong -fno-plt -O2 -ffunction-sections -pipe -isystem /workspaces/auth_capture_proxy/venv/include -fdebug-prefix-map=/home/conda/feedstock_root/build_artifacts/curl_split_recipe_1770892450256/work=/usr/local/src/conda/curl_split_recipe-8.18.0 -fdebug-prefix-map=/workspaces/auth_capture_proxy/venv=/usr/local/src/conda-prefix -DNDEBUG -D_FORTIFY_SOURCE=2 -O2 -isystem /workspaces/auth_capture_proxy/venv/include' 'LDFLAGS=-Wl,-O2 -Wl,--sort-common -Wl,--as-needed -Wl,-z,relro -Wl,-z,now -Wl,--disable-new-dtags -Wl,--gc-sections -Wl,--allow-shlib-undefined -Wl,-rpath,/workspaces/auth_capture_proxy/venv/lib -Wl,-rpath-link,/workspaces/auth_capture_proxy/venv/lib -L/workspaces/auth_capture_proxy/venv/lib' 'CPPFLAGS=-DNDEBUG -D_FORTIFY_SOURCE=2 -O2 -isystem /workspaces/auth_capture_proxy/venv/include' 'CPP=/home/conda/feedstock_root/build_artifacts/curl_split_recipe_1770892450256/_build_env/bin/x86_64-conda-linux-gnu-cpp'"
    ;;

  *)
    echo "unknown option: $1"
    usage 1
    ;;
  esac
  shift
done

exit 0
